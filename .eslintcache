ate Hooks\nimport {\n  useRecoilValue as useValue,\n  useRecoilState as useRecoil,\n} from \"recoil\";\n//Global State\nimport {\n  todoState,\n  priorityState,\n  addState,\n  changeState,\n  removeState,\n} from \"../../state/atoms\";\nimport styles from \"./Todos.module.scss\";\n// Components\nimport TodoItem from \"../TodoItem\";\nimport TodoForm from \"../TodoForm\";\n\n// COMPONENT BODY\nexport default function Todos() {\n  // Global State Hooks\n  const [todoList, setTodoList] = useRecoil(todoState);\n  const changing = useValue(changeState);\n  const show = useValue(addState);\n  const removing = useValue(removeState);\n  const priority = useValue(priorityState);\n\n  // Sort TodoList\n  useEffect(() => {\n    if (priority) {\n      setTodoList(\n        todoList.slice().sort(function (a, b) {\n          return a.priority - b.priority; // By priority\n        })\n      );\n    } else {\n      setTodoList(\n        todoList.slice().sort(function (a, b) {\n          return a.title.localeCompare(b.title); // By name\n        })\n      );\n    }\n    // Watches priority for sortButton, and editing, adding and deleting modes etc.\n  }, [priority, changing, show, removing]);\n\n  return (\n    <div className={styles.todos}>\n      {/* Dynamically render  todo items */}\n      {todoList.map((todo, index) => (\n        <span key={todo.id} className={styles.todoItem}>\n          <TodoItem key={index} item={todo} />\n        </span>\n      ))}\n      {/* Todo Form only appears in adding mode */}\n      {show ? <TodoForm /> : null}\n    </div>\n  );\n}\n","/home/nath_king93/recoil-todo-app/src/components/SortButton/index.js",[],"/home/nath_king93/recoil-todo-app/src/components/Numbers/index.js",[],"/home/nath_king93/recoil-todo-app/src/state/atoms.js",[],"/home/nath_king93/recoil-todo-app/src/components/TodoItem/index.js",[],"/home/nath_king93/recoil-todo-app/src/components/TodoForm/index.js",[],"/home/nath_king93/recoil-todo-app/src/utils/Helpers.js",[],"/home/nath_ki